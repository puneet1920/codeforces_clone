<!-- left panel -->
<div class="col-9 pr-0">
    <!-- inner body navbar -->
    <% if(typeof(innerNav) != 'undefined' && innerNav != null){ %>
        <%- include('panel-view/left-panel/inner-body-nav-bar/inner-body-nav-bar') %>
    <% } %>
    <%- include('panel-view/left-panel/' + body[1]) %>
</div>

<!-- right panel -->
<div class="col-3" style="min-height: 60vh;">
    <% rightPanel.forEach(panel => { %>
        <%- include('panel-view/right-panel/' + panel.file, {panel : panel}) %>
    <% }) %>
</div>